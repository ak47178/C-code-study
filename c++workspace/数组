/*
 * 数组：
 *
 */
#include<iostream>
using namespace std;
#include<iomanip>
using std::setw;
int main(){
	int n[10]; // 定义数组
	// 初始化数组元素
	for(int i=0;i<10;i++){
		n[i] = i+100;
	}
	cout << "Element " << setw(13) << "value" << endl;

	// 打印数组中的每个元素的数据
	for(int j=0;j<10;j++){
		cout << setw(7)<<j<<setw(13)<<n[j]<<endl;
	}
	return 0;
}
