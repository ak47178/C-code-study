#include <iostream>
using namespace std;
// 变量声明
extern int a,b;
extern int c;
extern float f;

int main() {
	//变量定义
	int a,b;
	int c;
	float f;

	// 实际初始化
	a = 10;
	b = 20;
	c = a + b;
	cout << c << endl ;  // 输出c 的值
	f = 70.0/3.0;
	cout << f << endl ;  // 输出f的值
	return 0;
}
